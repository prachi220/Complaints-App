        </activity>
        <activity
            android:name=".UserMainActivity"
            android:label="@string/title_activity_user_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Categorised" />
        <activity android:name=".ComplaintActivity" />
        <activity android:name=".AdminMainActivity" />
        <activity android:name=".ManageActivity" />
        <activity android:name=".AddremActivity" />
        <activity android:name=".ChangeAuthActivity" />
